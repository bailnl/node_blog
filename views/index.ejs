<%- include header %>
<% posts.forEach(function (post, index) { %>
<div class="post">
    <h2><a href="#"><%= post.title %></a></h2>
    <div class="info">
        作者： <a href="#"><%= post.name %></a> |
        日期： <%= post.time.minute %>
    </div>
    <p>
        <%- post.post %>
    </p>
</div>
<% }) %>
<%- include footer %>




